{
  "name": "codedbyfj/nexora",
  "description": "Modern Frontend Framework & Section Builder for Bagisto.",
  "type": "bagisto-package",
  "license": "proprietary",
  "authors": [
    {
      "name": "CodedByFJ",
      "email": "support@codedbyfj.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "CodedByFJ\\Nexora\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CodedByFJ\\Nexora\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CodedByFJ\\Nexora\\Providers\\NexoraServiceProvider"
      ],
      "aliases": {}
    }
  }
}
